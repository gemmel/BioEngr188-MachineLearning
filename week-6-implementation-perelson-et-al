   "execution_count": 80,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The p-value of the KS test for Patient 104 = 1.0\n",
      "The p-value of the KS test for Patient 105 = 1.0\n",
      "The p-value of the KS test for Patient 107 = 1.0\n"
     ]
    }
   ],
   "source": [
    "# Run 2-sample KS test\n",
    "ks_2samp_104 = sp.stats.ks_2samp(Vt_104, v_104)\n",
    "print('The p-value of the KS test for Patient 104 =', ks_2samp_104[1])\n",
    "\n",
    "ks_2samp_105 = sp.stats.ks_2samp(Vt_105, v_105)\n",
    "print('The p-value of the KS test for Patient 105 =', ks_2samp_105[1])\n",
    "\n",
    "ks_2samp_107 = sp.stats.ks_2samp(Vt_107, v_107)\n",
    "print('The p-value of the KS test for Patient 107 =', ks_2samp_107[1])"
   ]

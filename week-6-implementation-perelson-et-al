    "VI_105 = 1.23e3*np.exp(-c_105*t)\n",
    "plt.semilogy(t,VI_105, 'k--')\n",
    "plt.xlim(0,8)\n",
    "plt.ylim(1e1, 1e4)\n",
    "plt.title('Pt. 105: V_I')\n",
    "plt.show()"
